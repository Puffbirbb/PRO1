+    public static int setValueAt(
+            int[][] numbers, int r, int c, int value
+    ){
+        numbers[r][c] = value;
+        return value;
+    }
+    public static int sumRow(int[][] numbers, int r){
+        int sum = 0;
+        for (int c = 0; c < numbers[r].length; c++){
